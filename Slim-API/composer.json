{
    "name": "vendor/a-p-i-slim-framework",
    "description": "Description of project API-SlimFramework.",
    "authors": [
        {
            "name": "Fernando Paschoeto",
            "email": "fernandopaschoeto@gmail.com"
        }
    ],
    "require": {
        "slim/slim": "3.*",
        "illuminate/database": "*",
        "slim/slim-skeleton": "*",
        "slim/php-view": "^2.0",
        "monolog/monolog": "^1.17",
        "firebase/php-jwt": "*"
    },
        "require-dev": {
        "phpunit/phpunit": ">=5.0"
    },
    "autoload": {
        "psr-4": {
            "MyApp\\": "src/MyApp",
            "Tests\\": "tests/",
            "App\\": "App/"
        }
    },
    "config": {
        "process-timeout": 0,
        "sort-packages": true
    },
    "scripts": {
        "start": "php -S localhost:8080 -t public",
        "test": "phpunit"
    }
}